    "def map_age_bin(age):\n",
    "    if age < 18:\n",
    "        return '0-18'\n",
    "    if age < 35:\n",
    "        return '18-35'\n",
    "    elif age < 55:\n",
    "        return '35-60'\n",
    "    elif not math.isnan(age):\n",
    "        return '> 60'\n",
    "    \n",
    "titanic_data['Age Bin'] = titanic_data['Age'].apply(map_age_bin)\n",
    "titanic_data.head()"
   ]
